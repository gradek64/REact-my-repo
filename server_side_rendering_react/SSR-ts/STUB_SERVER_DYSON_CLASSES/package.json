{
  "name": "react_web",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "concurrently --raw --kill-others \"singleSession:run:stub\" \"npm:dev:*\"",
    "multi": "concurrently --raw --kill-others  \"npm:multiSession:run:stub\" \"npm:dev:*\"",
    "dev:build-server": "webpack -w --config ./config/server/webpack.config.dev.js",
    "---wait---": "wait on server to be build before running it",
    "dev:run-server": "wait-on ./dist/server.js &&  nodemon --inspect  ./dist/server.js",
    "---INFO Stubs---": "runs stub server on localhost:3011",
    "singleSession:run:stub": "NODE_ENV=stub nodemon --watch ./stubs --exec ts-node ./stubs/start-stub.js --ext ts,js --ignore ./stubs/sessions",
    "---INFO multiSession Stubs---": "runs localhost:3011 in multisession mode with session cookie",
    "multiSession:run:stub": "NODE_ENV=stub MULTIPLE_SESSIONS=true nodemon --watch ./stubs --exec ts-node ./stubs/start-stub.js --ext ts,js --ignore ./stubs/sessions",
    "---INFO---": "need to open on http://localhost:3100/ which hydreated react client app not server",
    "dev:run:client-dev-server": "webpack-dev-server --hot --config ./config/client/webpack.config.js --mode development --port 3100"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/preset-env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import",
      "react-loadable/babel"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@reduxjs/toolkit": "1.3.6",
    "@sainsburys-tech/boltui-utils": "./sainsburys-local/boltui-utils",
    "@swc/core": "^1.2.205",
    "@types/express": "^4.17.13",
    "@types/react": "^16.8.5",
    "@types/react-loadable": "^5.5.6",
    "@zxcvbn-ts/core": "^2.0.1",
    "@zxcvbn-ts/language-common": "^2.0.1",
    "@zxcvbn-ts/language-en": "^2.0.1",
    "boxen": "4.2.0",
    "concurrently": "^6.3.0",
    "connected-react-router": "6.6.1",
    "cross-env": "^6.0.3",
    "dayjs": "1.8.20",
    "dyson": "^2.0.4",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "handlebars": "^4.7.7",
    "isomorphic-fetch": "^3.0.0",
    "nodemon": "^2.0.13",
    "path-to-regexp": "^6.2.1",
    "react": "16.8.5",
    "react-content-loader": "^6.1.0",
    "react-dom": "16.8.5",
    "react-loadable": "^5.5.0",
    "react-redux": "7.1.3",
    "react-router-dom": "^5.3.0",
    "serialize-javascript": "^6.0.0",
    "session-file-store": "^1.3.0",
    "superagent": "3.8.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-loader": "8.0.5",
    "babel-regenerator-runtime": "^6.5.0",
    "chalk": "3.0.0",
    "css-loader": "^6.8.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "html-loader": "0.5.5",
    "node-color-log": "^6.0.0",
    "sass-loader": "10",
    "style-loader": "2",
    "typescript": "^4.9.3",
    "url-loader": "^2.2.0",
    "wait-on": "^7.0.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",

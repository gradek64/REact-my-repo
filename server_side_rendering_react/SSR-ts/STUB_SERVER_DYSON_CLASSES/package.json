{
  "name": "react_web",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "--------- START ----------------------": "",
    "RUN-STUBS-MULTI-session": "any env variable wil be passed to process.env so run : MULTIPLE_SESSIONS=true yarn start",
    "start": "yarn clean && cross-env NODE_ENV=stub yarn development",
    "start:devGateway": "cross-env NODE_ENV=development yarn development",
    "start:stagingGateway": "cross-env NODE_ENV=staging yarn development",
    "--------- DEVELOP ----------------------": "",
    "development": "export SET NODE_OPTIONS=--openssl-legacy-provider && concurrently --raw --kill-others \"npm:development:*\"",
    "---BUILD-RUN-SERVER---": "compiles and runs the server from './dist/server.js'",
    "development:build-server": "webpack -w --config ./config/server/webpack.config.dev.js",
    "---wait-on_server---": "wait on server to be build in webpack before running it",
    "development:run-server": "wait-on ./dist/server.js &&  nodemon --inspect  ./dist/server.js",
    "---Stub Server---": "runs stub server on localhost:3011",
    "development:run:stub": "NODE_ENV=stub nodemon --watch ./stubs --exec ts-node ./stubs/start-stub.js --ext ts,js --ignore ./stubs/sessions",
    "---Dev-front-end server--": "need to open on http://localhost:3100/ which hydreated react client app not server",
    "development:run:client-dev-server": "webpack-dev-server --hot --config ./config/client/webpack.config.js --mode development --port 3100",
    "--------- LINTING ----------------------": "",
    "lint": "yarn lint:scss && yarn lint:js",
    "lint:scss": "stylelint \"**/*.{js,jsx,ts,tsx}\" --ignore-path .eslintignore",
    "lint:js": "yarn lint:javascript && yarn lint:ts && yarn lint:gherkin && yarn lint:json",
    "lint:javascript": "eslint --ext .jsx --ext .js src/ cypress/",
    "lint:ts": "eslint --ext .tsx --ext .ts ./ && tsc",
    "--------- HELPERS ----------------------": "",
    "bundlesize": "yarn build && bundlesize --config config/bundlesize/.bundlesizeconfig.json",
    "write:bundlesize": "node ./config/bundlesize/bundlesizer.js",
    "clean": "rimraf dist reports",
    "nuke": "yarn cache clean && yarn global add rimraf && rimraf node_modules && yarn && yarn run clean"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/preset-env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import",
      "react-loadable/babel"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",

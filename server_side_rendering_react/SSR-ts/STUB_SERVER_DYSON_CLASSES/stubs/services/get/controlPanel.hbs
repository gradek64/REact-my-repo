  <script>
    $(document).ready(function () {
      $('.changeStub').change(function () {
        let value = $(this).val()
        if ($(this).attr('type') === 'checkbox') {
          value = $(this).is(':checked')
        } else if ($(this).attr('type') === 'radio') {
          value = $(`input[name=${$(this).attr('name')}]:checked`).val()
        }
        $.ajax({
          type: 'GET',
          url: '/payment-stub/' + $(this).data('object'),
          data: {
            type: $(this).data('type'),
            value,
            index: $(this).data('index')
          },
          success: function () {
            location.reload()
          }
        })
      })
      $('.clickStub').click(function () {
        const isForm = $(this).parent().is('form')
        $.ajax({
          type: 'GET',
          url: '/payment-stub/' + $(this).data('object'),
          data: {
            type: $(this).data('type'),
            value: isForm ? $(this).parent().serialize() : $(this).data('value'),
            index: $(this).data('index')
          },
          success: function () {
            location.reload()
          }
        })
      })
      $("#slider-range").slider({
      range: true,
      min: 0,
      max: 100,
      values: [ $("#minimumDuration").val(), $("#maximumDuration").val() ],
      slide: function(event, ui) {
        $("#amount").val(`${ui.values[0]} - ${ui.values[1]}`);
        $("#minimumDuration").val(ui.values[0])
        $("#maximumDuration").val(ui.values[1])
      }
    });
    $("#amount").val(
      $("#slider-range").slider("values",0) + " - " + $("#slider-range").slider("values",1) 
      );
    })
  </script>

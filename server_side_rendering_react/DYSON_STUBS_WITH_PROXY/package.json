{
  "name": "react_web",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "concurrently --raw --kill-others \"npm:dev:*\"",
    "dev:build-server": "webpack -w --config ./config/server/webpack.config.dev.js",
    "wait": "wait on server to be build before running it",
    "dev:run-server": "wait-on ./dist/server.js &&  nodemon --inspect  ./dist/server.js",
    "STUBS": "stub server is running on 3011  port",
    "dev:stub:sessions": "cross-env MULTIPLE_SESSIONS=true node --max-http-header-size=81920 ./stubs/start-stub.js",
    "---INFO---": "need to open on http://localhost:3100/ which hydreated react client app not server",
    "dev:run:client-dev-server": "webpack-dev-server --hot --config ./config/client/webpack.config.js --mode development --port 3100",
    "TESTING": "e2e",
    "test:e2e:focus": "cypress open",
    "test:e2e:all": "cypress run -e 'TAGS=not @crossOrigin'"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "babel-regenerator-runtime": "^6.5.0",
    "concurrently": "^6.3.0",
    "connected-react-router": "^6.9.1",
    "consul": "^0.39.0",
    "cross-env": "^5.2.0",
    "dyson": "^2.0.3",
    "express": "^4.17.1",
    "express-http-proxy": "1.6.0",
    "express-request-proxy": "2.2.2",
    "express-session": "^1.15.6",
    "handlebars": "4.0.12",
    "history": "^4.10.1",
    "isomorphic-fetch": "^3.0.0",
    "nconf": "^0.10.0",
    "nodemon": "^2.0.13",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "serialize-javascript": "^6.0.0",
    "session-file-store": "1.2.0",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "7.0.0",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.3",
    "@cypress/webpack-preprocessor": "^5.16.3",
    "babel-loader": "8.0.5",
    "boxen": "4.2.0",
    "css-loader": "2.1.1",
    "cypress": "^12.5.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "node-color-log": "^6.0.0",
    "prop-types": "15.6.2",
    "style-loader": "^0.23.1",
    "url-loader": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
